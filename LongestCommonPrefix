String strs[] = { "flower", "flow", "flight" };
		String prefix = "";
		Arrays.sort(strs);

		String first = strs[0];
		String last = strs[strs.length - 1];

		for (int i = 0; i < first.length(); i++) {

			if (first.charAt(i) == last.charAt(i)) {
				prefix = prefix + first.charAt(i);
			}

		}

		System.out.println(prefix);
